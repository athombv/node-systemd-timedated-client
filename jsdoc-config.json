{
    "opts": {
        "template": "templates/default",
        "encoding": "utf8",
        "recurse": true,
        "destination": "./docs/"
    },
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["index.js", "README.md"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_",
        "exclude": "test"
    },
    "plugins": [],
    "templates": {
        "default": {
            "outputSourceFiles": false
        }
    }
}